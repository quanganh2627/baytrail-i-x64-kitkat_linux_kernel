			};
			goa-setting-8 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB8 0xD2 0x00 0x00>;
				intel,cmd-lp = <1>;
			};
			goa-setting-9 {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xB9 0x90>;
