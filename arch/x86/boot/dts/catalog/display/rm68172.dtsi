			vcl-boosting {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB8 0x24 0x24 0x24>;
				intel,cmd-lp = <1>;
			};
			vgh-boosting {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB9 0x34 0x34 0x34>;
				intel,cmd-lp = <1>;
			};
			vgl-boosting {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xBA 0x04 0x04 0x04>;
				intel,cmd-lp = <1>;
			};
			gamma-voltage-1 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xBC 0x00 0x78 0x00>;
