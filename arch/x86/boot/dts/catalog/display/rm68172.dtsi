				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xC0 0x00 0x01 0xFA 0x00>;
				intel,cmd-lp = <1>;
			};
			enable-page-0 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xF0 0x55 0xAA 0x52 0x08 0x00>;
				intel,cmd-lp = <1>;
			};
			rgb-porch-1 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB0 0x00 0x10 0x10>;
				intel,cmd-lp = <1>;
			};
			rgb-porch-2 {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xB1 0xFC>;
				intel,cmd-lp = <1>;
			};
			ce-on {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xB4 0x10>;
				intel,cmd-lp = <1>;
			};
			src-ctrl-porch {
				intel,cmd-type = <0x15>;
				intel,cmd-data = <0xBA 0x01>;
				intel,cmd-lp = <1>;
			};
			inversion-drv-ctrl {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xBC 0x02 0x02 0x02>;
