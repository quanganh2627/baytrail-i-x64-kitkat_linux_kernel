				intel,cmd-data = <0xB0 0x0F 0x0F 0x0F>;
				intel,cmd-lp = <1>;
			};
			avee {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB1 0x08 0x08 0x08>;
				intel,cmd-lp = <1>;
			};
			avdd-boosting {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xB6 0x34 0x34 0x34>;
				intel,cmd-lp = <1>;
