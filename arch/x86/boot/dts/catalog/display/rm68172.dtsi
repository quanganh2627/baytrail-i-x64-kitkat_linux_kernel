	rm68172 {
		compatible = "intel,display";
		intel,display-resolution= <480 800>;
		intel,display-dpi= <235 235>;

		/* MIPI-DSI video mode settings */
		intel,display-vid-hfp-hs-bytes = <20>;
		intel,display-vid-hbp-hs-bytes = <20>;
		intel,display-vid-hsa-hs-bytes = <4>;
		intel,display-vid-vfp-lines = <16>;
		intel,display-vid-vbp-lines = <16>;
		intel,display-vid-vsa-lines = <2>;

		intel,display-vid-mode = "burst";
		intel,display-vid-id = <0>;
		intel,display-vid-pixel = "24packed";

		intel,display-reset = <1 1 0 5 1 50>;

		cmd-init {
			enable-page-2 {
				intel,cmd-type = <0x39>;
				intel,cmd-data = <0xF0 0x55 0xAA 0x52 0x08 0x02>;
				intel,cmd-lp = <1>;
			};
			osc {
